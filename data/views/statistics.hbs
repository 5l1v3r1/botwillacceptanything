<center>
    <table>
        <tr>
            <td>Webhook Status:</td>
            <td>Disabled</td>
        </tr>
        <tr>
            <td>Operating System:</td>
            <td>{{os.type}} - {{os.release}}</td>
        </tr>
        <tr>
            <td>System Uptime:</td>
            <td>{{os.uptime}} Seconds</td>
        </tr>
        <tr>
            <td>System Name:</td>
            <td>{{os.hostname}}</td>
        </tr>
        <tr>
            <td>Webhook Status:</td>
            {{#if config.githubAuth.webhookSecret}}
                Enabled
            {{else}}
                Disabled
            {{/if}}
        </tr>
    </table>
</center>